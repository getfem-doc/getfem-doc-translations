msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:375
msgid "``RIDs = gf_mesh_get(mesh M, 'regions')``"
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:377
msgid "Return the list of valid regions stored in the mesh."
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:380
msgid "``RIDs = gf_mesh_get(mesh M, 'boundary')``"
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:382
msgid "DEPRECATED FUNCTION. Use 'region' instead."
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:385
msgid "``CVFIDs = gf_mesh_get(mesh M, 'region', ivec RIDs)``"
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:387
msgid "Return the list of convexes/faces on the regions `RIDs`."
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:389
msgid "`CVFIDs` is a two-rows matrix, the first row lists convex #ids, and the second lists face numbers (local number in the convex). (and 0 when the whole convex is in the regions)."
msgstr ""

#: ../../getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_get.rst:395
