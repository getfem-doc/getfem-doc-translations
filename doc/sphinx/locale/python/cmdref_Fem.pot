#: ../source/python/cmdref_Fem.rst:8
msgid "Fem"
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:1
msgid "GetFEM Fem object"
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:3
msgid "This object represents a finite element method on a reference element."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:6
msgid "General constructor for Fem objects"
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:8
msgid "``F = Fem('interpolated_fem', MeshFem mf_source, MeshIm mim_target, [ivec blocked_dofs[, bool caching]])`` Build a special Fem which is interpolated from another MeshFem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:11
#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:22
msgid "Using this special finite element, it is possible to interpolate a given MeshFem `mf_source` on another mesh, given the integration method `mim_target` that will be used on this mesh."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:15
#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:26
msgid "Note that this finite element may be quite slow or consume much memory depending if caching is used or not. By default `caching` is True"
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:19
msgid "``F = Fem('projected_fem', MeshFem mf_source, MeshIm mim_target, int rg_source, int rg_target[, ivec blocked_dofs[, bool caching]])`` Build a special Fem which is interpolated from another MeshFem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:30
msgid "``F = Fem(string fem_name)`` The `fem_name` should contain a description of the finite element method. Please refer to the GetFEM manual (especially the description of finite element and integration methods) for a complete reference. Here is a list of some of them:"
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:36
msgid "FEM_PK(n,k) : classical Lagrange element Pk on a simplex of dimension `n`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:38
msgid "FEM_PK_DISCONTINUOUS(n,k[,alpha]) : discontinuous Lagrange element Pk on a simplex of dimension `n`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:40
msgid "FEM_QK(n,k) : classical Lagrange element Qk on quadrangles, hexahedrons etc."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:42
msgid "FEM_QK_DISCONTINUOUS(n,k[,alpha]) : discontinuous Lagrange element Qk on quadrangles, hexahedrons etc."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:44
msgid "FEM_Q2_INCOMPLETE(n) : incomplete Q2 elements with 8 and 20 dof (serendipity Quad 8 and Hexa 20 elements)."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:47
msgid "FEM_PK_PRISM(n,k) : classical Lagrange element Pk on a prism of dimension `n`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:49
msgid "FEM_PK_PRISM_DISCONTINUOUS(n,k[,alpha]) : classical discontinuous Lagrange element Pk on a prism."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:51
msgid "FEM_PK_WITH_CUBIC_BUBBLE(n,k) : classical Lagrange element Pk on a simplex with an additional volumic bubble function."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:54
msgid "FEM_P1_NONCONFORMING : non-conforming P1 method on a triangle."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:56
msgid "FEM_P1_BUBBLE_FACE(n) : P1 method on a simplex with an additional bubble function on face 0."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:58
msgid "FEM_P1_BUBBLE_FACE_LAG : P1 method on a simplex with an additional lagrange dof on face 0."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:60
msgid "FEM_PK_HIERARCHICAL(n,k) : PK element with a hierarchical basis."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:62
msgid "FEM_QK_HIERARCHICAL(n,k) : QK element with a hierarchical basis."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:64
msgid "FEM_PK_PRISM_HIERARCHICAL(n,k) : PK element on a prism with a hierarchical basis."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:66
msgid "FEM_STRUCTURED_COMPOSITE(Fem f,k) : Composite Fem `f` on a grid with `k` divisions."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:68
msgid "FEM_PK_HIERARCHICAL_COMPOSITE(n,k,s) : Pk composite element on a grid with `s` subdivisions and with a hierarchical basis."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:71
msgid "FEM_PK_FULL_HIERARCHICAL_COMPOSITE(n,k,s) : Pk composite element with `s` subdivisions and a hierarchical basis on both degree and subdivision."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:74
msgid "FEM_PRODUCT(A,B) : tensorial product of two polynomial elements."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:76
msgid "FEM_HERMITE(n) : Hermite element P3 on a simplex of dimension `n = 1, 2, 3`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:78
msgid "FEM_ARGYRIS : Argyris element P5 on the triangle."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:80
msgid "FEM_HCT_TRIANGLE : Hsieh-Clough-Tocher element on the triangle (composite P3 element which is C1), should be used with IM_HCT_COMPOSITE() integration method."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:84
msgid "FEM_QUADC1_COMPOSITE : Quadrilateral element, composite P3 element and C1 (16 dof)."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:86
msgid "FEM_REDUCED_QUADC1_COMPOSITE : Quadrilateral element, composite P3 element and C1 (12 dof)."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:88
msgid "FEM_RT0(n) : Raviart-Thomas element of order 0 on a simplex of dimension `n`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:90
msgid "FEM_NEDELEC(n) : Nedelec edge element of order 0 on a simplex of dimension `n`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem:93
msgid "Of course, you have to ensure that the selected fem is compatible with the geometric transformation: a Pk fem has no meaning on a quadrangle."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.base_value:1
msgid "Evaluate all basis functions of the FEM at point `p`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.base_value:3
#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.grad_base_value:3
#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.hess_base_value:3
msgid "`p` is supposed to be in the reference convex!"
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.char:1
msgid "Ouput a (unique) string representation of the Fem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.char:3
msgid "This can be used to perform comparisons between two different Fem objects."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.dim:1
msgid "Return the dimension (dimension of the reference convex) of the Fem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.display:1
msgid "displays a short summary for a Fem object."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.estimated_degree:1
msgid "Return an estimation of the polynomial degree of the Fem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.estimated_degree:3
msgid "This is an estimation for fem which are not polynomials."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.grad_base_value:1
msgid "Evaluate the gradient of all base functions of the Fem at point `p`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.hess_base_value:1
msgid "Evaluate the Hessian of all base functions of the Fem at point `p`."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.index_of_global_dof:1
msgid "Return the index of global dof for special fems such as interpolated fem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.is_equivalent:1
msgid "Return 0 if the Fem is not equivalent."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.is_equivalent:3
msgid "Equivalent Fem are evaluated on the reference convex. This is the case of most classical Fem's."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.is_lagrange:1
msgid "Return 0 if the Fem is not of Lagrange type."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.is_polynomial:1
msgid "Return 0 if the basis functions are not polynomials."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.nbdof:1
msgid "Return the number of dof for the Fem."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.nbdof:3
msgid "Some specific Fem (for example 'interpolated_fem') may require a convex number `cv` to give their result. In most of the case, you can omit this convex number."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.poly_str:1
msgid "Return the polynomial expressions of its basis functions in the reference convex."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.poly_str:4
msgid "The result is expressed as a tuple of strings. Of course this will fail on non-polynomial Fem's."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.pts:1
msgid "Get the location of the dof on the reference element."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.pts:3
msgid "Some specific Fem may require a convex number `cv` to give their result (for example 'interpolated_fem'). In most of the case, you can omit this convex number."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.target_dim:1
msgid "Return the dimension of the target space."
msgstr ""

#: ../../../../../../venv/lib/python3.8/site-packages/getfem/getfem.py:docstring of getfem.getfem.Fem.target_dim:3
msgid "The target space dimension is usually 1, except for vector Fem."
msgstr ""
