#: ../source/python/cmdref_Integ.rst:8
msgid "Integ"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:1
msgid "GeFEM Integ object"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:3
msgid "General object for obtaining handles to various integrations methods on convexes (used when the elementary matrices are built)."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:7
msgid "General constructor for Integ objects"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:9
msgid "``I = Integ(string method)`` Here is a list of some integration methods defined in getfem++ (see the description of finite element and integration methods for a complete reference):"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:14
msgid "IM_EXACT_SIMPLEX(n) : Exact integration on simplices (works only with linear geometric transformations and PK Fem's)."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:17
msgid "IM_PRODUCT(A,B) : Product of two integration methods."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:19
msgid "IM_EXACT_PARALLELEPIPED(n) : Exact integration on parallelepipeds."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:21
msgid "IM_EXACT_PRISM(n) : Exact integration on prisms."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:23
msgid "IM_GAUSS1D(k) : Gauss method on the segment, order `k=1,3,...,99`."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:25
msgid "IM_NC(n,k) : Newton-Cotes approximative integration on simplexes, order `k`."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:27
msgid "IM_NC_PARALLELEPIPED(n,k) : Product of Newton-Cotes integration on parallelepipeds."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:29
msgid "IM_NC_PRISM(n,k) : Product of Newton-Cotes integration on prisms."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:31
msgid "IM_GAUSS_PARALLELEPIPED(n,k) : Product of Gauss1D integration on parallelepipeds."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:33
msgid "IM_TRIANGLE(k) : Gauss methods on triangles `k=1,3,5,6,7,8,9,10,13,17,19`."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:35
msgid "IM_QUAD(k) : Gauss methods on quadrilaterons `k=2,3,5, ...,17`. Note that IM_GAUSS_PARALLELEPIPED should be prefered for QK Fem's."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:38
msgid "IM_TETRAHEDRON(k) : Gauss methods on tetrahedrons `k=1,2,3,5,6 or 8`."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:40
msgid "IM_SIMPLEX4D(3) : Gauss method on a 4-dimensional simplex."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:42
msgid "IM_STRUCTURED_COMPOSITE(im,k) : Composite method on a grid with `k` divisions."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:44
msgid "IM_HCT_COMPOSITE(im) : Composite integration suited to the HCT composite finite element."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:47
msgid "Example:"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:49
msgid "I = Integ('IM_PRODUCT(IM_GAUSS1D(5),IM_GAUSS1D(5))')"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:51
msgid "is the same as:"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:53
msgid "I = Integ('IM_GAUSS_PARALLELEPIPED(2,5)')"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ:55
msgid "Note that 'exact integration' should be avoided in general, since they only apply to linear geometric transformations, are quite slow, and subject to numerical stability problems for high degree Fem's."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.char:1
msgid "Ouput a (unique) string representation of the integration method."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.char:3
msgid "This can be used to  comparisons between two different Integ objects."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.coeffs:1
msgid "Returns the coefficients associated to each integration point."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.coeffs:3
#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.face_coeffs:3
#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.face_pts:3
#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.nbpts:6
#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.pts:3
msgid "Only for approximate methods, this has no meaning for exact integration methods!"
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.dim:1
msgid "Return the dimension of the reference convex of the method."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.display:1
msgid "displays a short summary for a Integ object."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.face_coeffs:1
msgid "Returns the coefficients associated to each integration of a face."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.face_pts:1
msgid "Return the list of integration points for a face."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.is_exact:1
msgid "Return 0 if the integration is an approximate one."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.nbpts:1
msgid "Return the total number of integration points."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.nbpts:3
msgid "Count the points for the volume integration, and points for surface integration on each face of the reference convex."
msgstr ""

#: ../../../../../../../virtualenv/python3.6.10/lib/python3.6/site-packages/getfem/__init__.py:docstring of getfem.Integ.pts:1
msgid "Return the list of integration points"
msgstr ""
