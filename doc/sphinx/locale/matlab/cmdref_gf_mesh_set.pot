msgstr ""

#: /home/travis/build/getfem-doc/getfem-doc-translations/getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_set.rst:105
msgid "``gf_mesh_set(mesh M, 'transform', mat T)``"
msgstr ""

#: /home/travis/build/getfem-doc/getfem-doc-translations/getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_set.rst:107
msgid "Applies the matrix `T` to each point of the mesh."
msgstr ""

#: /home/travis/build/getfem-doc/getfem-doc-translations/getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_set.rst:109
msgid "Note that `T` is not required to be a ``NxN`` matrix (with ``N = gf_mesh_get(mesh M, 'dim')``). Hence it is possible to transform a 2D mesh into a 3D one (and reciprocally)."
msgstr ""

#: /home/travis/build/getfem-doc/getfem-doc-translations/getfem/doc/sphinx/source/matlab/cmdref_gf_mesh_set.rst:114
msgid "``gf_mesh_set(mesh M, 'boundary', int rnum, mat CVFIDs)``"
msgstr ""

