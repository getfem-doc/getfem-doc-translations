#: ../source/userdoc/appendixA.rst:1038
msgid ""
"\\begin{array}{ll}\n"
"\\widehat{\\varphi}_0 = (1-x-y)(1+x+y-2x^2-2y^2-11xy),~~ & (\\widehat{\\varphi}_0(0,0) = 1), \\\\\n"
"\\widehat{\\varphi}_1 = x(1-x-y)(1-x-2y), & (\\partial_x\\widehat{\\varphi}_1(0,0) = 1), \\\\\n"
"\\widehat{\\varphi}_2 = y(1-x-y)(1-2x-y), & (\\partial_y\\widehat{\\varphi}_2(0,0) = 1), \\\\\n"
"\\widehat{\\varphi}_3 = -2x^3 + 7 x^2y + 7xy^2 + 3x^2 - 7xy, & (\\widehat{\\varphi}_3(1,0) = 1), \\\\\n"
"\\widehat{\\varphi}_4 = x^3-2x^2y-2xy^2-x^2+2xy, & (\\partial_x\\widehat{\\varphi}_4(1,0) = 1), \\\\\n"
"\\widehat{\\varphi}_5 = xy(y+2x-1), & (\\partial_y\\widehat{\\varphi}_5(1,0) = 1), \\\\\n"
"\\widehat{\\varphi}_6 = 7x^2y + 7xy^2 - 2y^3+3y^2-7xy, & (\\widehat{\\varphi}_6(0,1) = 1), \\\\\n"
