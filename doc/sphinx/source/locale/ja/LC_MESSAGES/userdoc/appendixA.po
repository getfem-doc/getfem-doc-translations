msgstr "形状関数の読み込みは次の通りです．"

#: ../source/userdoc/appendixA.rst:1378
msgid ""
"\\begin{array}{l}\n"
"\\widehat{\\varphi}_{0}(x,y,z) = \\dfrac{\\xi_0 \\xi_1}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_0)(1-\\xi_4-2\\xi_1) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{1}(x,y,z) = 4\\dfrac{\\xi_0\\xi_1\\xi_2}{(1-\\xi_4)^2}(2\\xi_1-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{2}(x,y,\\xi_4) = \\dfrac{\\xi_1 \\xi_2}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_1)(1-\\xi_4-2\\xi_2) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{3}(x,y,z) = 4\\dfrac{\\xi_3\\xi_0\\xi_1}{(1-\\xi_4)^2}(2\\xi_0-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{4}(x,y,z) = 16\\dfrac{\\xi_0\\xi_1\\xi_2\\xi_3}{(1-\\xi_4)^2}, \\\\\n"
"\\widehat{\\varphi}_{5}(x,y,z) = 4\\dfrac{\\xi_1\\xi_2\\xi_3}{(1-\\xi_4)^2}(2\\xi_2-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{6}(x,y,z) = \\dfrac{\\xi_3 \\xi_0}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_3)(1-\\xi_4-2\\xi_0) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{7}(x,y,z) = 4\\dfrac{\\xi_2\\xi_3\\xi_0}{(1-\\xi_4)^2}(2\\xi_3-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{8}(x,y,z) = \\dfrac{\\xi_2 \\xi_3}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_2)(1-\\xi_4-2\\xi_3) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{9}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_0\\xi_1, \\\\\n"
"\\widehat{\\varphi}_{10}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_1\\xi_2,  \\\\\n"
"\\widehat{\\varphi}_{11}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_3\\xi_0,  \\\\\n"
"\\widehat{\\varphi}_{12}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_2\\xi_3,  \\\\\n"
"\\widehat{\\varphi}_{13}(x,y,z) = \\xi_4(2\\xi_4-1). \\\\\n"
"\\end{array}"
msgstr ""
"\\begin{array}{l}\n"
"\\widehat{\\varphi}_{0}(x,y,z) = \\dfrac{\\xi_0 \\xi_1}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_0)(1-\\xi_4-2\\xi_1) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{1}(x,y,z) = 4\\dfrac{\\xi_0\\xi_1\\xi_2}{(1-\\xi_4)^2}(2\\xi_1-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{2}(x,y,\\xi_4) = \\dfrac{\\xi_1 \\xi_2}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_1)(1-\\xi_4-2\\xi_2) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{3}(x,y,z) = 4\\dfrac{\\xi_3\\xi_0\\xi_1}{(1-\\xi_4)^2}(2\\xi_0-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{4}(x,y,z) = 16\\dfrac{\\xi_0\\xi_1\\xi_2\\xi_3}{(1-\\xi_4)^2}, \\\\\n"
"\\widehat{\\varphi}_{5}(x,y,z) = 4\\dfrac{\\xi_1\\xi_2\\xi_3}{(1-\\xi_4)^2}(2\\xi_2-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{6}(x,y,z) = \\dfrac{\\xi_3 \\xi_0}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_3)(1-\\xi_4-2\\xi_0) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{7}(x,y,z) = 4\\dfrac{\\xi_2\\xi_3\\xi_0}{(1-\\xi_4)^2}(2\\xi_3-(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{8}(x,y,z) = \\dfrac{\\xi_2 \\xi_3}{(1-\\xi_4)^2}((1-\\xi_4-2\\xi_2)(1-\\xi_4-2\\xi_3) -\\xi_4(1-\\xi_4)), \\\\\n"
"\\widehat{\\varphi}_{9}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_0\\xi_1, \\\\\n"
"\\widehat{\\varphi}_{10}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_1\\xi_2,  \\\\\n"
"\\widehat{\\varphi}_{11}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_3\\xi_0,  \\\\\n"
"\\widehat{\\varphi}_{12}(x,y,z) = 4\\dfrac{\\xi_4}{1-\\xi_4}\\xi_2\\xi_3,  \\\\\n"
"\\widehat{\\varphi}_{13}(x,y,z) = \\xi_4(2\\xi_4-1). \\\\\n"
"\\end{array}"

